{
  "name": "nexa-ui-components",
  "version": "1.0.2",
  "description": "React UI components library with Material-UI",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./Alert": {
      "types": "./dist/Alert/index.d.ts",
      "import": "./dist/Alert/index.js"
    },
    "./Button": {
      "types": "./dist/Button/index.d.ts",
      "import": "./dist/Button/index.js"
    },
    "./Breadcrumbs": {
      "types": "./dist/Breadcrumbs/index.d.ts",
      "import": "./dist/Breadcrumbs/index.js"
    },
    "./Cropper": {
      "types": "./dist/Cropper/index.d.ts",
      "import": "./dist/Cropper/index.js"
    },
    "./theme": {
      "types": "./dist/theme.d.ts",
      "import": "./dist/theme.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "test": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build": "pnpm run clean && pnpm run build:swc && pnpm run build:types",
    "build:swc": "swc src -d dist --copy-files --ignore '**/*.stories.tsx,**/*.test.tsx,**/*.spec.tsx' --strip-leading-paths",
    "build:types": "tsc --project tsconfig.build.json --emitDeclarationOnly",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run type-check && pnpm run build",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "material-ui",
    "nexa"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dawid-Femin/nexaui"
  },
  "packageManager": "pnpm@10.20.0",
  "pnpm": {
    "overrides": {
      "fsevents": "2.3.3"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "fsevents"
      ]
    }
  },
  "peerDependencies": {
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@mui/material": "^7.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "react-easy-crop": "^5.5.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@chromatic-com/storybook": "^4.1.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/material-icons": "^5.2.7",
    "@fontsource/roboto": "^5.2.8",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@storybook/addon-a11y": "^10.0.6",
    "@storybook/addon-docs": "^10.0.6",
    "@storybook/addon-links": "^10.0.6",
    "@storybook/addon-onboarding": "^10.0.6",
    "@storybook/addon-themes": "^10.0.6",
    "@storybook/addon-vitest": "^10.0.6",
    "@storybook/nextjs": "^10.0.6",
    "@storybook/react-vite": "^10.0.6",
    "@storybook/test": "^8.6.14",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitest/browser-playwright": "^4.0.8",
    "@vitest/coverage-v8": "^4.0.8",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "publint": "^0.3.15",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "storybook": "^10.0.6",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  }
}